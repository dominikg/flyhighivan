{
	"name": "flyhighivan",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "run-p -c check:*",
		"check:types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:lint": "eslint --cache --ignore-path .gitignore '**/*.{js,ts,svelte}'",
		"check:format": "prettier --cache --ignore-path .gitignore . --ignore-unknown --check",
		"check:inlang": "inlang lint",
		"lint": "pnpm check:lint --fix",
		"format": "pnpm check:format --write",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@inlang/cli": "^0.13.3",
		"@inlang/plugin-json": "^3.0.12",
		"@inlang/plugin-standard-lint-rules": "^3.0.0",
		"@inlang/sdk-js": "^0.11.8",
		"@inlang/sdk-js-plugin": "^0.11.8",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.24.1",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"husky": "^8.0.0",
		"lint-staged": "^14.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"sanitize.css": "^13.0.0",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"tiny-glob": "^0.2.9",
		"tslib": "^2.6.2",
		"typescript": "~5.2.2",
		"vite": "^4.4.9"
	},
	"type": "module",
	"packageManager": "pnpm@8.7.4",
	"engines": {
		"node": ">=18"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint --cache --fix",
		"*": "prettier --cache --plugin prettier-plugin-svelte --ignore-unknown --write"
	}
}
